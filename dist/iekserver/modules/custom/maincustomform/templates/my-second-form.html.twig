{% include 'common-part.html.twig' %}
  <h2 class="form-title">Προσωπικά Στοιχεία</h2>                  
  <div class="row">
    <div class="col-md-6">
      {{ form.last }}
    </div>
    <div class="col-md-6">
      {{ form.first }}
    </div>
    <div class="col-md-6">
    {{ form.fname }}
    </div>
    <div class="col-md-6">
      {{ form.mname }}
    </div>
    </div>
    <div class="row">
    <div class="col-md-6">
      {{ form.idno }}
    </div>
    <div class="col-md-6">
      {{ form.afm }}
