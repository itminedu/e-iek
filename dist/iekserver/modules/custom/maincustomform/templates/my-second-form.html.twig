      {{ form.birth_date }}
    </div>
    <div class="col-md-6">
      {{ form.birth_place }}
    </div>
    </div>
   <div class="row">
    <div class="col-md-6">
      {{ form.email }}
    </div>
    <div class="col-md-6">
      {{ form.telephone }}
    </div>
